{
  "name": "script-editor",
  "version": "0.1.0",
  "description": "",
  "main": "main/main.js",
  "scripts": {
    "watch": "parcel renderer/index.html --public-url ./ --target electron --no-hmr",
    "build": "parcel build renderer/index.html --public-url ./ --target electron --no-content-hash",
    "start": "electron .",
    "start:forge": "electron-forge start",
    "rebuild:arm64": "electron-rebuild --arch arm64",
    "package": "electron-forge package",
    "make": "electron-forge make --targets @electron-forge/maker-zip",
    "format": "prettier --write '**/*.{ts,tsx,js,json}'",
    "clean": "rm -rf dist",
    "lint": "eslint \"renderer/**/*.{ts,tsx}\"",
    "lint:fix": "npm run format && eslint --fix \"renderer/**/*.{ts,tsx}\"",
    "check": "tsc --noEmit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.3",
    "@types/react": "^18.0.4",
    "@types/react-dom": "^18.0.0",
    "@types/wavesurfer.js": "^6.0.2",
    "@types/web-bluetooth": "^0.0.15",
    "date-fns": "^2.28.0",
    "immutability-helper": "^3.1.1",
    "react": "^18.0.0",
    "react-dnd": "^16.0.0",
    "react-dnd-html5-backend": "^16.0.0",
    "react-dom": "^18.0.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.0",
    "wavesurfer.js": "^6.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "electron": "^21.0.0",
    "electron-rebuild": "^3.2.7",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  }
}
